<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<!--
	RSyntaxTextArea's default theme.
	See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

   <!-- Omitting baseFont will use a system-appropriate monospaced. -->
   <!--<baseFont family="..." size="13"/>-->
   
   <!--  General editor colors. -->
   <background color="ffffff"/>
   <caret color="424242"/>
   <selection fg="ffffff" bg="4a90d9" />
   <currentLineHighlight color="ffffaa" fade="false"/>
   <marginLine fg="b0b4b9"/>
   <markAllHighlight color="ffc800"/>
   <markOccurrencesHighlight color="d4d4d4" border="false"/>
   <matchedBracket fg="000080" bg="ffffff" highlightBoth="false" animate="false"/>
   <hyperlinks fg="0000ff"/>
   <secondaryLanguages>
      <language index="1" bg="fff0cc"/>
      <language index="2" bg="dafeda"/>
      <language index="3" bg="ffe0f0"/>
   </secondaryLanguages>
   
   <!-- Gutter styling. -->
   <gutterBorder color="dddddd"/>
   <lineNumbers fg="787878"/>
   <foldIndicator fg="808080" iconBg="ffffff"/>
   <iconRowHeader activeLineRange="3399ff"/>
   
   <!-- Syntax tokens. -->
   <tokenStyles>
      <style token="IDENTIFIER" fg="D35400" bold="true"/>
      <style token="DATA_TYPE" fg="00979D" bold="false"/>
      <style token="FUNCTION" fg="728E00" bold="false"/>
      <style token="VARIABLE" fg="cc6600" bold="false"/>
      <style token="RESERVED_WORD" fg="D35400" bold="false"/>
      <style token="RESERVED_WORD_2" fg="00979D" bold="false"/>
      <style token="PREPROCESSOR" fg="660033" bold="false"/>

      <style token="ANNOTATION" fg="434f54" bold="false"/>
      <style token="COMMENT_DOCUMENTATION" fg="E2E2E2" bold="false"/>
      <style token="COMMENT_EOL" fg="7e7e7e" bold="false"/>
      <style token="COMMENT_MULTILINE" fg="7e7e7e" bold="false"/>
      <style token="COMMENT_KEYWORD" fg="7F9FBF" bold="true"/>
      <style token="COMMENT_MARKUP" fg="7f7f9f" bold="false"/>
      <style token="LITERAL_BOOLEAN" fg="00979D" bold="false"/>
      <style token="LITERAL_NUMBER_DECIMAL_INT" fg="706443" bold="false"/>
      <style token="LITERAL_NUMBER_FLOAT" fg="706443" bold="false"/>
      <style token="LITERAL_NUMBER_HEXADECIMAL" fg="706443" bold="false"/>
      <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="006699" bold="false"/>
      <style token="LITERAL_CHAR" fg="006699" bold="false"/>
      <style token="LITERAL_BACKQUOTE" fg="8A7B52" bold="false"/>

      <style token="OPERATOR" fg="728E00" bold="false"/>
      <style token="REGEX" fg="006699" bold="false"/>
      <style token="SEPARATOR" fg="000000" bold="false"/>
      <style token="WHITESPACE" fg="000000" bold="false"/>
      <style token="ERROR_IDENTIFIER" fg="000000" bold="false"/>
      <style token="ERROR_NUMBER_FORMAT" fg="000000" bold="false"/>
      <style token="ERROR_STRING_DOUBLE" fg="000000" bold="false"/>
      <style token="ERROR_CHAR" fg="000000" bold="false"/>    
   </tokenStyles>

</RSyntaxTheme>
